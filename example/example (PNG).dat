# Это комментарий

# Требуются ли погрешности по Ox, Oy
x_error: 0
y_error: 1

# Границы осей (опционально)
x_limits: 0 15
y_limits: 0 70

# Имя выходного файла
output: plot1.png

# Подписи осей
x_title: I, А
y_title: B, мТл

# При значении label=1 на графике отображается подпись с угловым коэффициентом
label: 1

# Координаты подписи на графике
label_x: 1
label_y: 60

# Единицы измерения
label_text: " мТл/А"
# Размер подписи, пока не работает в LaTeX TODO!!!
label_size: 20

# Выбор терминала для gnuplot, поддерживает опции
term: png size 1920, 1080

# Данные по колонкам
data:
